unit UClasse.Cript.password;

interface

uses UInterfaces;

type
  TMyClass = class(TInterfacedObject, i_interface)
  private
  public
    constructor create;
    destructor destroy; override;
    class function new: i_interface;
  end;

implementation

{ TMyClass }

constructor TMyClass.create;
begin

end;

destructor TMyClass.destroy;
begin

  inherited;
end;

class function TMyClass.new: i_interface;
begin
  result := self.create;
end;

end.
