unit UFactory;

interface

uses UInterfaces;

type
  TFactory = class(TInterfacedObject, iCriarJanelas)

  private
  public
    constructor create;
    destructor destroy; override;
    class function new: iCriarJanelas;

  end;

implementation

{ TFactory }

constructor TFactory.create;
begin

end;

destructor TFactory.destroy;
begin

  inherited;
end;

class function TFactory.new: iCriarJanelas;
begin
  result := self.create;
end;

end.
