unit UClasse.Config.BackUp;

interface

uses
  UClasse.Config.Acesso.Banco, System.SysUtils;

Type
  TClasseConfigBackUp = class
  private
    FQuery: TConfigConexaoBanco;
  public

    constructor create;
    destructor destroy; override;

  end;

implementation

{ TClasseConfigBackUp }

constructor TClasseConfigBackUp.create;
begin
  FQuery.retornarDados('config_backup');
end;

destructor TClasseConfigBackUp.destroy;
begin

  FreeAndNil(FQuery);

  inherited;
end;

end.
